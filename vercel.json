{
  "version": 2,
  "builds": [
    { "src": "html/*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/html/main.html" },
    { "src": "/event", "dest": "/html/event.html" },
    { "src": "/garage", "dest": "/html/garage.html" },
    { "src": "/news", "dest": "/html/news.html" },
    { "src": "/subscribe", "dest": "/html/subscribe.html" }
  ]
}
